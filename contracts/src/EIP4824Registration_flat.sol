        registration = Clones.cloneDeterministic(
            template,
            _saltedSalt(msg.sender, salt)
        );

        if (manager == address(0)) {
            EIP4824Registration(registration).initialize(
                msg.sender,
                daoURI_,
                eip4824Index
            );
        } else {
            EIP4824Registration(registration).initialize(
                msg.sender,
                manager,
                daoURI_,
                eip4824Index
